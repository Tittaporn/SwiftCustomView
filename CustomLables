
//  LablesRMS.swift

import UIKit

class Label20FontRMS: UILabel {
    override func awakeFromNib() {
        super.awakeFromNib()
        self.textColor = .darkGray
        self.font = .rms20
    }
}

class Label20BoldFontRMS: UILabel {
    override func awakeFromNib() {
        super.awakeFromNib()
        self.textColor = .darkGray
        self.font = .rms20?.bold()
    }
}

